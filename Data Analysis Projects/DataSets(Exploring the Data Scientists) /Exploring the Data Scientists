        "from nltk.tokenize import WhitespaceTokenizer as WsT\n",
        "from nltk.probability import *\n",
        "from itertools import chain\n",
        "import codecs\n",
        "from nltk.corpus import stopwords \n",
        "nltk.download('stopwords')"
      ],
