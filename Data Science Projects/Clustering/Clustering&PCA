      "source": [
        "pca_for_89 = PCA(n_components = 3)\n",
        "pca_for_89.fit(Xnorm)\n",
        "varnce_for_89 = pca_for_89.explained_variance_ratio_\n",
        "print(varnce_for_89)\n",
        "fit_for_89 = pca_for_89.fit_transform(Xnorm)"
      ],
      "execution_count": null,
