      "source": [
        "# Create Dummies for precip_type column  0 for snow and 1 for rain in both train & test data\n",
        "train_data_with_dummy = pd.get_dummies(train_data,drop_first=True)\n",
        "test_data_with_dummy = pd.get_dummies(test_data,drop_first=True)\n",
        "\n",
        "# get_dummies() function changes column name thus renaming to old one to keep consistency\n",
        "train_data_with_dummy.rename(columns={'precip_type_snow': 'precip_type',}, inplace=True)\n",
        "test_data_with_dummy.rename(columns={'precip_type_snow': 'precip_type',}, inplace=True)\n"
      ],
      "execution_count": 10,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "NxexaQxzZCkR",
        "colab_type": "code",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 34
        },
        "outputId": "21329b13-95d3-48ca-a336-b12403e043b8"
      },
      "source": [
        "# Creating a Regression Model \n",
        "from sklearn.linear_model import LinearRegression\n",
        "from sklearn.metrics import mean_absolute_error\n",
        "\n",
        "# Create the regressor: reg\n",
        "reg = LinearRegression()\n",
        "\n",
        "X_train = train_data_with_dummy.drop('temperature' , axis= 1).values\n",
        "y_train = train_data_with_dummy['temperature'].values\n",
        "\n",
        "X_test = test_data_with_dummy.drop('temperature' , axis= 1).values\n",
        "y_test = test_data_with_dummy['temperature'].values\n",
        "\n",
        "# Fit the regressor to the training data\n",
        "reg.fit(X_train,y_train)\n",
        "\n",
        "# Predict on the test data: y_pred\n",
        "y_pred = reg.predict(X_test)\n",
        "\n",
        "MAE = mean_absolute_error(y_test,y_pred)\n",
        "print(\" Mean Absolute Error: {}\".format(MAE))"
      ],
      "execution_count": 11,
      "outputs": [
