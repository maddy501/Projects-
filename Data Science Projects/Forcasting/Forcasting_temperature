      "source": [
        "from sklearn.metrics import mean_squared_error\n",
        "from sklearn.model_selection import train_test_split\n",
        "# Create Dummies for precip_type column  0 for snow and 1 for rain in both train & test data\n",
        "weather_data_scaled.precip_type[weather_data_scaled.precip_type == 'snow'] = 0\n",
        "weather_data_scaled.precip_type[weather_data_scaled.precip_type == 'rain'] = 1\n",
        "\n",
        "# Seperating features from predictor and dropping the recording_date_time column\n",
        "X = weather_data_scaled.drop(['recording_date_time','temperature'],axis = 1).values\n",
        "y = weather_data_scaled['temperature'].values"
      ],
      "execution_count": 13,
      "outputs": [
