      "source": [
        "# Loading the data into a variable\n",
        "weather_data = pd.read_csv('weather_dataset.csv')\n",
        "\n",
        "#Scaling the dataset using the scale function \n",
        "scaled_dataset = pd.DataFrame(scale(weather_data[['apparent_temperature','humidity','wind_speed','wind_bearing',\n",
        "                                                        'visibility', 'cloud_cover','pressure']]))\n",
        "# adding back the column names as scale function removes them\n",
        "scaled_dataset.columns = ('apparent_temperature','humidity','wind_speed','wind_bearing','visibility','cloud_cover',\n",
        "                             'pressure')\n",
        "weather_data_scaled = pd.concat([scaled_dataset,weather_data[['recording_date_time','temperature','precip_type']]],axis = 1)\n"
      ],
      "execution_count": 7,
      "outputs": []
    },
