        "# Filter and create Train and Test Data\n",
        "\n",
        "train_data = weather_data_scaled[weather_data_scaled['recording_date_time'] < \"2015-01-01\"]\n",
        "test_data =  weather_data_scaled[weather_data_scaled['recording_date_time'] >= \"2015-01-01\"]\n",
        "\n",
        "# Drop the recording_date_time and year column \n",
        "train_data = train_data.drop(['recording_date_time'] , axis=1)\n",
        "test_data = test_data.drop(['recording_date_time'], axis=1)\n",
        "\n",
        "# Print the shape of train and test data \n",
